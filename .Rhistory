rm_0s_by_col <- function(input_matrix) {
no_0s_matrix <- input_matrix[, colSums(input_matrix !=0)]
return(no_0s_matrix)}
rm_0s_by_row <- function(input_matrix) {
no_0_matrix <- input_matrix[rowSums(input_matrix[])>0,]
return(no_0_matrix)}
